{
  "manifest_version": 2,

  "name": "BypassAdPage",
  "description": "This extension bypasses the ad page from the any.gs domain.",
  "version": "1.0",
  
  "content_scripts": [
    {
      "matches": ["http://www.any.gs/*"],
      "js": ["content.js"],
	  "run_at":"document_start"
    }
  ],
    
  "icons": { "16": "icon16.png", "48": "icon48.png", "128": "icon128.png" }
    

}
